nota1 = float(input())
nota2 = float(input())
nota3 = float(input())
nota4 = float(input())
media = (nota1 + nota2 + nota3 + nota4) / 4
soma = nota1 + nota2 + nota3 + nota4
print("A média foi= ", end='', flush=True)
print(media)
print("a soma é= ", end='', flush=True)
print(soma)
while media >= 5:
print("APROVADO")
print("REPROVADO")
